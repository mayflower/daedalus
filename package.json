{
  "name": "daedalus",
  "productName": "Daedalus",
  "version": "0.8.0",
  "description": "Cryptocurrency Wallet",
  "main": "./dist/main.js",
  "scripts": {
    "lint": "eslint --format=node_modules/eslint-formatter-pretty app electron features *.js",
    "hot-server": "cross-env NODE_ENV=development node --preserve-symlinks -r babel-register webpack/server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack/webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack/webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./electron/main.development",
    "start-debug": "npm run start -- --debug=5858",
    "start-debug-hot": "npm run start-hot -- --debug=5858",
    "package": "cross-env NODE_ENV=production node -r babel-register -r babel-polyfill webpack/package.js",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "preinstall": "which electron || npm install electron@1.7.11",
    "dev": "npm run purge-translations && concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "debug": "concurrently --kill-others \"npm run hot-server\" \"npm run start-debug-hot\" \"electron --inspect-brk=9222 ./\"",
    "cleanup": "mop -v",
    "cucumber": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 cucumber-js --compiler js:babel-register --format 'progress-bar' --format-options '{\"snippetInterface\": \"synchronous\"}' --tags 'not @skip'",
    "cucumber-watch": "cross-env HOT=1 npm run cucumber -- --tags @watch",
    "test": "npm run cucumber",
    "test-watch": "nodemon --exec npm run cucumber-watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "flow-test": "flow; test $? -eq 0 -o $? -eq 2",
    "manage-translations": "node ./translations/translation-runner.js",
    "purge-translations": "rm -rf ./translations/messages/app",
    "build-dll": "webpack --config=webpack/webpack.config.dll.js"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.14",
    "@storybook/addon-links": "^3.3.14",
    "@storybook/addons": "^3.3.14",
    "@storybook/react": "^3.3.14",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "cucumber": "^3",
    "devtron": "^1.4.0",
    "electron": "1.7.11",
    "electron-devtools-installer": "^2.2.3",
    "electron-packager": "^11.0.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^4.1.0",
    "fbjs-scripts": "^0.8.1",
    "happypack": "^4.0.1",
    "mobx-react-devtools": "^4.2.15",
    "node-sass": "^4.7.2",
    "ps-tree": "^1.1.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "spectron": "3.7.2",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "webpack": "^3.11.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "aes-js": "2.1.1",
    "bignumber.js": "4.0.0",
    "bip39": "2.3.0",
    "blakejs": "1.0.1",
    "bs58": "4.0.1",
    "classnames": "2.2.5",
    "electron-debug": "1.1.0",
    "electron-log": "2.0.2",
    "electron-store": "1.3.0",
    "es6-error": "4.0.2",
    "humanize-duration": "3.12.0",
    "lodash": "4.17.4",
    "mkdirp": "0.5.1",
    "mobx": "3.1.7",
    "mobx-react": "4.1.5",
    "mobx-react-form": "1.32.2",
    "mobx-react-router": "3.1.2",
    "moment": "2.18.1",
    "pbkdf2": "3.0.14",
    "pdf.js-extract": "0.0.5",
    "qrcode.react": "0.6.1",
    "react": "15.4.2",
    "react-addons-css-transition-group": "15.4.2",
    "react-copy-to-clipboard": "4.2.3",
    "react-css-themr": "2.0.0",
    "react-dom": "15.4.2",
    "react-dropzone": "3.12.2",
    "react-intl": "2.2.3",
    "react-markdown": "2.5.0",
    "react-number-format": "1.1.2",
    "react-polymorph": "0.5.4",
    "react-router": "3.0.3",
    "react-svg-inline": "2.0.0",
    "recharts": "1.0.0-alpha.4",
    "route-parser": "0.0.5",
    "safe-buffer": "5.1.1",
    "source-map-support": "0.4.14",
    "unorm": "1.4.1",
    "validator": "6.3.0",
    "web3": "1.0.0-beta.24",
    "winston": "2.3.1",
    "winston-papertrail": "1.0.4"
  },
  "devEngines": {
    "node": "6.x",
    "npm": ">=3.x"
  }
}
